pipeline {
    agent any
    stages 
    {
        stage('Start') {
            steps {
                echo 'Starting'
                script{
                    def filePath = readFile "${WORKSPACE}/${INPUTFILE}"  
                    def lines = filePath.readLines() 
                    def branches = [:]
                    for (line in lines) { 
                        String[] str = line.split(',')
                        if str.length < 3 {
                            str[2] = "1"
                        }
                        branches["branch-${str[0]}"] = {
                            build job: 'gcs-bucket-mover', parameters: [
                                string(name: 'bucket_name', value:"${str[0]}"),
                                string(name: 'proj_id', value:"${str[1]}"),
                                string(name: 'DATA_TRANSFER_FLAG', value:"${str[2]}")]
                        }
                    }
                    parallel branches
                }
            }
        }
        stage('End') {
            steps {
                echo 'Bye'
            }
        }
    }
}
 
